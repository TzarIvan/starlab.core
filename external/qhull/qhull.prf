
win32{
    LIBS += -L$$PWD/lib/win32
}

macx{
    LIBS += -L$$PWD/lib/darwin11
}

unix:!macx{
    LIBS += -L$$PWD/lib/unix
}


CONFIG(debug, debug|release) {
    LIBS += -lqhullcpp_d
    LIBS += -lqhullstatic_pd
} else {
    LIBS += -lqhullcpp
    LIBS += -lqhullstatic_p
}

INCLUDEPATH += $$PWD/include 
INCLUDEPATH += $$PWD/include/libqhullcpp

DEFINES += qh_QHpointer
